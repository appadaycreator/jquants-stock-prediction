[run]
source = .
omit = 
    */tests/*
    */venv/*
    */__pycache__/*
    */node_modules/*
    */web-app/*
    */docs/*
    setup.py
    run_tests.py
    create_sample_data.py
    generate_web_data.py
    deploy.sh

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = J-Quants株価予測システム テストカバレッジレポート

[xml]
output = coverage.xml
