[run]
branch = True
omit =
    venv/*
    web-app/*
    node_modules/*
    docs/*
    htmlcov/*
    tests/*
    scripts/*
    generate_*.
    examples/*
    routine_api.py

[report]
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.

[html]
directory = htmlcov
title = J-Quants株価予測システム テストカバレッジレポート

[xml]
output = coverage.xml
